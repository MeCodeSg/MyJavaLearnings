//pat9
for(int i=1; i<=n; i++) {
    for(int j=1; j<=i-1; j++) {
        System.out.print(" \t");
    }
    System.out.print("*\t");
    for(int j=1; j<=2*(n-i)-1; j++) {
        System.out.print(" \t");
    }
    if(i!=n) {
        System.out.print("*\t");
    }
    System.out.println();
}
